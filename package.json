{
  "name": "linear-regression-utils",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@linear-regression-utils/web...",
    "dev:electron": "turbo run dev --filter=@linear-regression-utils/desktop...",
    "dev:all": "turbo run dev --parallel",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@linear-regression-utils/web...",
    "build:electron": "turbo run build --filter=@linear-regression-utils/desktop...",
    "start": "turbo run start",
    "start:web": "turbo run start --filter=@linear-regression-utils/web...",
    "start:electron": "turbo run start --filter=@linear-regression-utils/desktop...",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage"
  },
  "devDependencies": {
    "turbo": "latest",
    "typescript": "^5"
  }
}
